{
  "name": "rendezvous",
  "usedConcepts": {
    "authentication": {},
    "authorization": {},
    "comment": {},
    "event": {},
    "geolocation": {
      "config": {
        "mapType": "leaflet"
      }
    },
    "group": {},
    "relevantInterests": {
      "name": "label",
      "config": {
        "initialLabelIds": [
          "Sports", "Music", "Food & Drink", "Outdoor", "Theatre",
          "Culture & History"
        ]
      }
    },
    "eventDetails": {
      "name": "property",
      "config": {
        "schema": {
          "title": "EventDetails",
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "eventDetails": {
              "type": "string"
            }
          },
          "required": ["title"]
        }
      }
    },
    "userProfile": {
      "name": "property",
      "config": {
        "schema": {
          "title": "UserProfile",
          "type": "object",
          "properties": {
              "firstName": {
                "type": "string"
              },
              "middleName": {
                "type": "string"
              },
              "lastName": {
                "type": "string"
              },
              "phoneNumber": {
                "type": "string"
              },
              "zipCode": {
                "type": "number"
              },
              "majors": {
                "type": "string"
              },
              "email": {
                "type": "string",
                "format": "email"
              }
          },
          "required": ["firstName", "lastName", "zipCode", "email"]
        }
      }
    }
  },
  "routes": [
    { "path": "/main", "component": "main" },
    { "path": "/login", "component": "login" },
    { "path": "/register", "component": "register" },
    { "path": "/event-details", "component": "show-event-details" },
    { "path": "/create-event", "component": "create-event" },
    { "path": "/profile", "component": "show-user-profile" },
    { "path": "/update-profile", "component": "update-user-profile" },
    { "path": "", "component": "main" }
  ]
}
