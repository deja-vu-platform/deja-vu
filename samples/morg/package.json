{
  "name": "morg",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dv-start-gateway": "node node_modules/dv-gateway/dist/gateway.js --configFilePath dvconfig.json",
    "dv-build-morg": "ng build",
    "dv-build-watch-morg": "chokidar src node_modules/allocator node_modules/event -c 'if ! [[ \"{path}\" =~ node_modules/.*/.*]]; then ng build'; fi",
    "concurrently": "concurrently",
    "tsc": "tsc",
    "dv-package-event": "(cd ../../packages/catalog/event/; npm run dv-package-event)",
    "dv-package-watch-event": "(cd ../../packages/catalog/event/; npm run dv-package-watch-event)",
    "dv-start-event": "if [ -f node_modules/event/server/server.js ]; then node node_modules/event/server/server.js --config \"`dv get usedCliches.event.config`\"; else echo \"No file\"; fi;",
    "dv-start-watch-event": "if [ -f node_modules/event/server/server.js ]; then nodemon -w node_modules/event/server node_modules/event/server/server.js -- --config \"`dv get usedCliches.event.config`\"; else echo \"No file\"; fi;",
    "dv-reinstall-watch-event": "chokidar ../../packages/catalog/event/pkg -c \"npm run dv-reinstall-event\"",
    "dv-reinstall-event": "rm -rf node_modules/event && npm i",
    "dv-package-allocator": "(cd ../../packages/catalog/allocator/; npm run dv-package-allocator)",
    "dv-package-watch-allocator": "(cd ../../packages/catalog/allocator/; npm run dv-package-watch-allocator)",
    "dv-start-allocator": "if [ -f node_modules/allocator/server/server.js ]; then node node_modules/allocator/server/server.js --config \"`dv get usedCliches.allocator.config`\"; else echo \"No file\"; fi;",
    "dv-start-watch-allocator": "if [ -f node_modules/allocator/server/server.js ]; then nodemon -w node_modules/allocator/server node_modules/allocator/server/server.js -- --config \"`dv get usedCliches.allocator.config`\"; else echo \"No file\"; fi;",
    "dv-reinstall-watch-allocator": "chokidar ../../packages/catalog/allocator/pkg -c \"npm run dv-reinstall-allocator\"",
    "dv-reinstall-allocator": "rm -rf node_modules/allocator && npm i",
    "dv-start-all": "concurrently \"npm run dv-start-event\" \"npm run dv-start-allocator\" \"npm run dv-start-gateway\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.1",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.1",
    "@angular/material-moment-adapter": "^5.2.1",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "allocator": "file:../../packages/catalog/allocator/pkg",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "dv-core": "file:../../packages/dv-core/pkg",
    "dv-gateway": "file:../../packages/dv-gateway",
    "event": "file:../../packages/catalog/event/pkg",
    "moment": "^2.20.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "chokidar-cli": "^1.2.0",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.14.12",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
