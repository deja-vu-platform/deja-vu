{
  "name": "lingua",
  "usedConcepts": {
    "authentication": {
      "config": {
        "reinitDbOnStartup": false
      }
    },
    "authorization": {
      "config": {
        "reinitDbOnStartup": false
      }
    },
    "userProfile": {
      "name": "property",
      "config": {
        "reinitDbOnStartup": false,
        "schema": {
          "title": "Profile",
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "email": {
              "type": "string"
            },
            "aboutMe": {
              "type": "string"
            },
            "learningLanguage": {
              "type": "string",
              "enum": ["English", "French"]
            }
          }
        }
      }
    },
    "chat": {
      "config": {
        "reinitDbOnStartup": false
      }
    },
    "chatGroup": {
      "name": "group",
      "config": {
        "reinitDbOnStartUp": false
      }
    },
    "correction": {
      "name": "property",
      "config": {
        "schema": {
          "title": "Correction",
          "type": "object",
          "properties": {
            "originalText": {
              "type": "string"
            },
            "revisedText": {
              "type": "string"
            },
            "comment": {
              "type": "string"
            },
            "authorId": {
              "type": "string"
            },
            "chatId": {
              "type": "string"
            }
          }
        }
      }
    },
    "rating": {
      "config": {
        "reinitDbOnStartup": false
      }
    },
    "chatTotal": {
      "name": "group",
      "config": {
        "reinitDbOnStartUp": false
      }
    }
  },
  "routes": [
    { "path": "", "component": "show-chat" },
    { "path": "/login", "component": "login" },
    { "path": "/signup", "component": "signup" },
    { "path": "/profile", "component": "profile" },
    { "path": "/users", "component": "users"},
    { "path": "/chat", "component": "show-chat"}
  ]
}
