{
  "name": "dv-access-auth",
  "version": "0.0.1",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "tsc:app": "tsc app.ts db.ts --module commonjs",
    "tslint": "tslint *.ts auth/**/**/*.ts auth/**/*.ts auth/*.ts",
    "app:start": "node app.js",
