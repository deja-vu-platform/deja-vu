{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "@@name"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist/app",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "@@name",
      "styles": [
        "styles.scss"
      ],
      "scripts": []
    }
  ],
  "defaults": {
    "styleExt": "scss",
    "component": {
    },
    "build": {
      "preserveSymlinks": true
    }
  }
}
