{
  "name": "dv-cli",
  "version": "0.0.1",
  "description": "CLI tool for Déjà Vu",
  "main": "./dist/dv.js",
  "bin": {
    "dv": "./dist/dv.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && copyfiles -f src/*.blueprint.* dist",
    "clean": "rm -rf ./dist",
    "reinstall-globally": "npm run clean && npm run build && npm uninstall -g && npm install -g"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spderosso/dejavu.git"
  },
  "author": "Santiago De Rosso",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/spderosso/dejavu/issues"
  },
  "homepage": "https://github.com/spderosso/dejavu#readme",
  "dependencies": {
    "commander": "^2.13.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "copyfiles": "^1.2.0",
    "@types/lodash": "^4.14.96",
    "@types/node": "^9.3.0",
    "typescript": "~2.5.3"
  }
}
