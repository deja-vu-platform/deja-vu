{
  "name": "@deja-vu/schematics",
  "version": "0.0.1",
  "description": "Schematics for Déjà Vu",
  "scripts": {
    "build": "tsc -p tsconfig.json && rsync -r --include '*/files/' --exclude '.*' src/ dist/",
    "clean": "rm -rf ./dist",
    "package": "yarn clean && yarn build",
    "test": "yarn package && jasmine dist/**/*_spec.js"
  },
  "keywords": [
    "Deja Vu",
    "Déjà Vu",
    "dv",
    "schematics"
  ],
  "license": "MIT",
  "repository": "github:spderosso/dejavu",
  "schematics": "./dist/collection.json",
  "dependencies": {
    "@angular-devkit/core": "7.3.2",
    "@angular-devkit/schematics": "^7.3.2",
    "@types/cheerio": "^0.22.10",
    "@types/jasmine": "^3.0.0",
    "@types/node": "^8.0.31",
    "cheerio": "^1.0.0-rc.2",
    "jasmine": "^3.3.0",
    "typescript": "~3.2.2"
  }
}
