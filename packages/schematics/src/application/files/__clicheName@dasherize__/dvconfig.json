{
  "name": "<%= dasherize(clicheName) %>",
  "config": {
    "wsPort": 3001
  },
  "gateway": {
    "config": {
      "wsPort": 3000
    }
  },
  "components": {
    "package": {
      "include": [ "src/app/<%= dasherize(clicheName) %>/**" ]
    },
    "app": {
      "names": [
        { "for": "src/app/app.component.html", "use": "<%= dasherize(clicheName) %>-root" }
      ]
    }
  },
  "componentsRequestOptional": [
    "<%= dasherize(clicheName) %>-show-<%= dasherize(clicheName) %>"
  ]
}
