{
  "name": "@deja-vu/cli",
  "version": "0.0.5",
  "description": "CLI tool for Déjà Vu",
  "bin": {
    "dv": "./dist/dv.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/dv.js",
    "package": "npm run clean && npm run build",
    "lint": "tslint -p . -c tslint.json src/**/*.ts",
    "clean": "rm -rf ./dist ./pkg",
    "prepare": "npm run package"
  },
  "dependencies": {
    "@compodoc/compodoc": "^1.1.9",
    "fs-extra": "^6.0.1",
    "glob": "^7.1.2",
    "lodash": "^4.17.4",
    "posthtml": "^0.11.3",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@deja-vu/compiler": "^0.0.5",
    "@types/fs-extra": "^5.0.2",
    "@types/glob": "^5.0.35",
    "@types/lodash": "ts2.5",
    "@types/node": "^11.13.4",
    "prettier": "^1.18.2",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  },
  "peerDependencies": {
    "@deja-vu/compiler": "^0.0.5"
  },
  "repository": "github:spderosso/dejavu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spderosso/dejavu/issues"
  },
  "homepage": "https://github.com/spderosso/dejavu#readme",
  "types": "./dist/dv.d.ts",
  "main": "./dist/dv.js",
  "publishConfig": {
    "access": "public"
  }
}
