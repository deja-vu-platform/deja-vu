{
  "name": "ranking",
  "gateway": {
    "config": {
      "wsPort": 3000
    }
  },
  "config": {
    "wsPort": 3001
  },
  "components": {
    "package": {
      "include": [ "src/app/ranking/**" ]
    },
    "app": {
      "names": [
        { "for": "src/app/app.component.html", "use": "ranking-root" }
      ]
    }
  },
  "componentsRequestOptional": [
    "ranking-show-fractional-ranking",
    "ranking-show-ranking",
    "ranking-show-rankings"
  ]
}
