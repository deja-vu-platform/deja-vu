{
  "name": "market",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run dv-start-all",
    "build": "npm run dv-build-market",
    "package": "npm run dv-package-market",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dv-start-gateway": "nodemon node_modules/dv-gateway/dist/gateway.js --configFilePath dvconfig.json",
    "dv-build-market": "ng build && (cd server; tsc  && cp schema.graphql ../dist/server)",
    "dv-build-watch-market": "concurrently  \"chokidar src node_modules -c 'ng build'\" \"(cd server; tsc -w && cp schema.graphql ../dist/server)\"",
    "concurrently": "concurrently",
    "tsc": "tsc",
    "dv-package-market-client": "ng-packagr -p ng-package.json",
    "dv-package-market-server": "tsc -p server --outDir ./pkg/server && cp server/schema.graphql ./pkg/server",
    "dv-package-market": "npm run dv-package-market-client && npm run dv-package-market-server",
    "dv-package-watch-market": "chokidar src/app/market server -c 'npm run dv-package-market'",
    "dv-start-market": "if [ -f dist/server/server.js ]; then node dist/server/server.js  --config \"`dv get config`\"; else echo \"No file\"; fi;",
    "dv-start-watch-market": "if [ -f dist/server/server.js ]; then nodemon -w dist/server dist/server/server.js -- --config \"`dv get config`\"; else echo \"No file\"; fi;",
    "dv-start-all": "concurrently \"npm run dv-start-gateway\" \"npm run dv-start-watch-market\""
  },
  "private": true,
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "graphql": "^0.13.1",
    "graphql-tools": "^2.21.0",
    "minimist": "^1.2.0",
    "mongodb": "^3.0.4"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.4",
    "@angular/cli": "~1.7.3",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/minimist": "^1.2.0",
    "@types/mongodb": "^3.0.7",
    "@types/node": "~6.0.60",
