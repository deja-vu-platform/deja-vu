    "dv-core": "file:../../dv-core/pkg",
    "dv-gateway": "file:../../dv-gateway",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
