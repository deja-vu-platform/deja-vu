{
  "name": "@deja-vu/geolocation",
  "version": "0.0.2",
  "scripts": {
    "start": "dv serve",
    "build": "npm run dv-build-geolocation",
    "prepare": "npm run package",
    "package": "dv package",
    "clean": "rm -rf dist pkg pkg.tgz",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test-debug": "ng test --no-watch -sm=false",
    "lint": "ng lint --type-check; tslint -p . -c tslint.json server/*.ts",
    "lint-fix": "ng lint --type-check --fix; tslint --fix -p . -c tslint.json server/*.ts",
    "e2e": "ng e2e",
    "dv-build-geolocation": "ng build && tsc -p server && cp server/schema.graphql ./dist/server",
    "dv-package-geolocation-client": "ng-packagr -p ng-package.json",
    "dv-package-geolocation": "npm run dv-package-geolocation-client && npm run dv-package-geolocation-server",
    "dv-package-geolocation-server": "tsc -p server --outDir ./pkg/server && cp server/schema.graphql ./pkg/server",
    "concurrently": "concurrently",
    "tsc": "tsc",
    "ng": "ng",
    "dv": "dv"
  },
  "dependencies": {
    "@agm/core": "1.0.0-beta.3",
