{
  "name": "geolocation",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run dv-start-all",
    "build": "npm run dv-build-geolocation",
    "package": "npm run dv-package-geolocation",
    "test": "ng test",
    "lint": "./node_modules/.bin/tslint -p . -c tslint.json src/app/**/**/*.ts server/*.ts",
    "e2e": "ng e2e",
    "dv-start-gateway": "node node_modules/dv-gateway/dist/gateway.js --configFilePath dvconfig.json",
    "dv-build-geolocation": "ng build && (cd server && tsc  && cp schema.graphql ../dist/server)",
    "dv-build-watch-geolocation": "concurrently  \"chokidar src node_modules -c 'ng build'\" \"(cd server && tsc -w && cp schema.graphql ../dist/server)\"",
    "concurrently": "concurrently",
    "tsc": "tsc",
    "dv-package-geolocation-client": "ng-packagr -p ng-package.json",
    "dv-package-geolocation": "npm run dv-package-geolocation-client && npm run dv-package-geolocation-server",
    "dv-package-geolocation-server": "tsc -p server --outDir ./pkg/server && cp server/schema.graphql ./pkg/server",
    "dv-package-watch-geolocation": "chokidar src/app/geolocation server -c 'npm run dv-package-geolocation'",
    "dv-start-geolocation": "bash bin/start-geolocation.sh",
    "dv-start-watch-geolocation": "bash bin/start-watch-geolocation.sh",
    "dv-start-all": "concurrently \"npm run dv-start-gateway\" \"npm run dv-start-geolocation\""
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.2",
    "apollo-server-express": "^1.3.4",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.23.1",
    "minimist": "^1.2.0",
    "mongodb": "^3.0.5"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.1",
    "@angular/cli": "1.6.5",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.1",
    "@angular/language-service": "^5.2.0",
    "@angular/material-moment-adapter": "^5.2.1",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "dv-core": "file:../../dv-core/pkg",
    "dv-gateway": "file:../../dv-gateway",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/minimist": "^1.2.0",
    "@types/mongodb": "^3.0.5",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "chokidar-cli": "^1.2.0",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.0.0",
    "nodemon": "^1.14.12",
    "protractor": "~5.1.2",
