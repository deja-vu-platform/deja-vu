{
  "name": "geolocation",
  "config": {
    "wsPort": 3001
  },
  "gateway": {
    "config": {
      "wsPort": 3000
    }
  },
  "actions": {
    "package": {
      "include": [ "src/app/geolocation/**" ]
    },
    "app": {
      "names": [
        { "for": "src/app/app.component.html", "use": "geolocation-root" }
      ]
    }
  },
  "actionsRequestOptional": [
    "geolocation-show-marker",
    "geolocation-show-marker-count",
    "geolocation-show-markers"
  ]
}
