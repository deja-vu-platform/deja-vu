{
  "name": "comment",
  "gateway": {
    "config": {
      "wsPort": 3000
    }
  },
  "config": {
    "wsPort": 3001
  },
  "actions": {
    "package": {
      "include": [ "src/app/comment/**" ]
    },
    "app": {
      "names": [
        { "for": "src/app/app.component.html", "use": "comment-root" }
      ]
    }
  },
  "actionsRequestOptional": [
    "comment-show-comment",
    "comment-show-comment-count"
  ]
}
