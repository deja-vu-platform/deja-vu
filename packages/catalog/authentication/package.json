{
  "name": "authentication",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run dv-start-all",
    "build": "npm run dv-build-authentication",
    "package": "npm run dv-package-authentication",
    "test": "ng test",
    "lint": "./node_modules/.bin/tslint -p . -c tslint.json src/app/**/**/*.ts server/*.ts",
    "e2e": "ng e2e",
    "dv-start-gateway": "node node_modules/dv-gateway/dist/gateway.js --configFilePath dvconfig.json",
    "dv-build-authentication": "ng build && (cd server && tsc  && cp schema.graphql ../dist/server)",
    "dv-build-watch-authentication": "concurrently  \"chokidar src node_modules -c 'ng build'\" \"(cd server && tsc -w && cp schema.graphql ../dist/server)\"",
    "concurrently": "concurrently",
    "tsc": "tsc",
    "dv-package-authentication-client": "ng-packagr -p ng-package.json",
    "dv-package-authentication": "npm run dv-package-authentication-client && npm run dv-package-authentication-server",
    "dv-package-authentication-server": "tsc -p server --outDir ./pkg/server && cp server/schema.graphql ./pkg/server",
    "dv-package-watch-authentication": "chokidar src/app/authentication server -c 'npm run dv-package-authentication'",
    "dv-start-authentication": "bash bin/start-authentication.sh",
    "dv-start-watch-authentication": "bash bin/start-watch-authentication.sh",
    "dv-start-all": "concurrently \"npm run dv-start-gateway\" \"npm run dv-start-authentication\""
  },
  "private": true,
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.23.1",
    "jsonwebtoken": "^8.2.0",
    "minimist": "^1.2.0",
    "mongodb": "^3.0.4"
  },
