{
  "name": "property",
  "watch": true,
  "gatewayPort": 3000,
  "config": {
    "wsPort": 3001,
    "schema": {
      "title": "Person",
      "type": "object",
      "properties": {
          "firstName": {
              "type": "string"
          },
          "lastName": {
              "type": "string"
          },
          "age": {
              "description": "Age in years",
              "type": "integer",
              "minimum": 0
          }
      },
      "required": ["firstName", "lastName"]
    }
  },
  "startServer": true
}
