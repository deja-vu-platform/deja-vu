{
  "name": "event",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dv-start-gateway": "node node_modules/dv-gateway/dist/gateway.js --configFilePath dvconfig.json",
    "dv-build-event": "ng build && (cd server; tsc)",
    "dv-build-watch-event": "concurrently  \"chokidar src node_modules -c 'ng build'\" \"(cd server; tsc -w)\"",
    "concurrently": "concurrently",
    "tsc": "tsc",
    "packagr": "ng-packagr -p ng-package.json",
    "dv-package-event": "dv package",
    "dv-package-watch-event": "chokidar src/app/event server -c 'npm run dv-package-event'",
    "dv-start-event": "if [ -f dist/server/server.js ]; then node dist/server/server.js -- --config \"`dv get config`\"; else echo \"No file\"; fi;",
    "dv-start-watch-event": "if [ -f dist/server/server.js ]; then nodemon -w dist/server dist/server/server.js -- --config \"`dv get config`\"; else echo \"No file\"; fi;"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/minimist": "^1.2.0",
    "@types/mongodb": "^3.0.5",
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "core-js": "^2.4.1",
    "dv-core": "file:../../dv-core/pkg",
    "dv-gateway": "file:../../dv-gateway",
    "express": "^4.16.2",
    "graphql": "^0.13.0",
    "graphql-tools": "^2.21.0",
    "minimist": "^1.2.0",
    "mongodb": "^3.0.2",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "chokidar-cli": "^1.2.0",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.0.0",
    "nodemon": "^1.14.12",
