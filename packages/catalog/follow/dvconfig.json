{
  "name": "follow",
  "gateway": {
    "config": {
      "wsPort": 3000
    }
  },
  "config": {
    "wsPort": 3001
  },
  "components": {
    "package": {
      "include": [ "src/app/follow/**" ]
    },
    "app": {
      "names": [
        { "for": "src/app/app.component.html", "use": "follow-root" }
      ]
    }
  },
  "componentsRequestOptional": [
    "follow-show-follower-count",
    "follow-show-followers",
    "follow-show-message-count",
    "follow-show-messages",
    "follow-show-publisher-count",
    "follow-show-publishers"
  ]
}
