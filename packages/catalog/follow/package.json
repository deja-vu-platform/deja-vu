{
  "name": "follow",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "dv serve",
    "build": "npm run dv-build-follow",
    "package": "dv package",
    "test": "ng test",
    "lint": "./node_modules/.bin/tslint -p . -c tslint.json src/app/**/**/*.ts server/*.ts",
    "e2e": "ng e2e",
    "dv-start-gateway": "node node_modules/dv-gateway/dist/gateway.js --configFilePath dvconfig.json",
    "dv-build-follow": "ng build && (cd server && tsc  && cp schema.graphql ../dist/server)",
    "dv-build-watch-follow": "concurrently  \"chokidar src node_modules -c 'ng build'\" \"(cd server && tsc -w && cp schema.graphql ../dist/server)\"",
    "concurrently": "concurrently",
    "tsc": "tsc",
    "dv-package-follow-client": "ng-packagr -p ng-package.json",
    "dv-package-follow": "npm run dv-package-follow-client && npm run dv-package-follow-server",
    "dv-package-follow-server": "tsc -p server --outDir ./pkg/server && cp server/schema.graphql ./pkg/server",
    "dv-start-follow": "bash bin/start-follow.sh",
    "dv-start-watch-follow": "bash bin/start-watch-follow.sh",
    "dv-start-all": "concurrently \"npm run dv-start-gateway\" \"npm run dv-start-follow\""
  },
  "private": true,
  "peerDependencies": {
    "@angular/animations": "^5.2.0",
