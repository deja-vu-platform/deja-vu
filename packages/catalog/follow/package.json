    "@angular/router": "^5.2.0",
    "bootstrap": "^4.0.0",
    "cliche-server": "file:../../cliche-server",
    "core-js": "^2.4.1",
    "dv-core": "file:../../dv-core/pkg",
    "dv-gateway": "file:../../dv-gateway",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "dependencies": {
    "uuid": "^3.2.1"
  },
