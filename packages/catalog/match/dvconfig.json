{
  "name": "match",
  "config": {
    "wsPort": 3001
  },
  "gateway": {
    "config": {
      "wsPort": 3000
    }
  },
  "actions": {
    "package": {
      "include": [ "src/app/match/**" ]
    },
    "app": {
      "names": [
        { "for": "src/app/app.component.html", "use": "match-root" }
      ]
    }
  },
  "actionsRequestOptional": [
    "match-show-attempt",
    "match-show-attempts",
    "match-show-match",
    "match-show-matches"
  ]
}
