{
  "name": "@deja-vu/passkey",
  "version": "0.0.5",
  "scripts": {
    "start": "dv serve",
    "build": "npm run dv-build-passkey",
    "prepare": "npm run package",
    "package": "dv package",
    "clean": "rm -rf dist pkg pkg.tgz",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test-debug": "ng test --no-watch -sm=false",
    "lint": "ng lint --type-check; tslint -p . -c tslint.json server/*.ts",
    "lint-fix": "ng lint --type-check --fix; tslint --fix -p . -c tslint.json server/*.ts",
    "e2e": "ng e2e",
    "dv-build-passkey": "ng build && tsc -p server && cp server/schema.graphql ./dist/server",
    "dv-package-passkey-client": "ng-packagr -p ng-package.json",
    "dv-package-passkey": "npm run dv-package-passkey-client && npm run dv-package-passkey-server",
    "dv-package-passkey-server": "tsc -p server --outDir ./pkg/server && cp server/schema.graphql ./pkg/server",
    "concurrently": "concurrently",
    "tsc": "tsc",
    "ng": "ng",
    "dv": "dv"
  },
  "dependencies": {
    "jsonwebtoken": "^8.2.0",
    "sha.js": "^2.4.11"
  },
