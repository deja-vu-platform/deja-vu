{
  "name": "label",
  "watch": true,
  "gateway": {
    "config": {
      "wsPort": 3000
    }
  },
  "config": {
    "wsPort": 3001
  },
  "startServer": true,
  "actions": {
    "package": {
      "include": [ "src/app/label/**" ]
    },
    "app": {
      "names": [
        { "for": "src/app/app.component.html", "use": "label-root" }
      ]
    }
  }
}
