{
  "name": "scoring",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "dv serve",
    "build": "npm run dv-build-scoring",
    "package": "dv package",
    "test": "ng test",
    "lint": "./node_modules/.bin/tslint -p . -c tslint.json src/app/**/**/*.ts server/*.ts",
    "e2e": "ng e2e",
    "dv-start-gateway": "node node_modules/dv-gateway/dist/gateway.js --configFilePath dvconfig.json",
    "dv-build-scoring": "ng build && (cd server && tsc && cp schema.graphql ../dist/server)",
    "dv-build-watch-scoring": "chokidar src node_modules -c 'npm run dv-build-scoring'",
    "concurrently": "concurrently",
    "tsc": "tsc",
    "dv-package-scoring-client": "ng-packagr -p ng-package.json",
    "dv-package-scoring": "npm run dv-package-scoring-client && npm run dv-package-scoring-server",
    "dv-package-scoring-server": "tsc -p server --outDir ./pkg/server && cp server/schema.graphql ./pkg/server",
    "dv-package-watch-scoring": "chokidar src/app/scoring server -c 'npm run dv-package-scoring'",
    "dv-start-scoring": "bash bin/start-scoring.sh",
    "dv-start-watch-scoring": "bash bin/start-watch-scoring.sh",
    "dv-start-all": "concurrently \"npm run dv-start-gateway\" \"npm run dv-start-scoring\""
  },
  "private": true,
  "dependencies": {
    "apollo-server-express": "^1.3.5",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.24.0",
    "minimist": "^1.2.0",
    "mongodb": "^3.0.7"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.4",
    "@angular/cli": "~1.7.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/minimist": "^1.2.0",
    "@types/mongodb": "^3.0.15",
    "@types/node": "~6.0.60",
    "bootstrap": "^4.0.0",
    "chokidar-cli": "^1.2.0",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "core-js": "^2.4.1",
    "dv-core": "file:../../dv-core/pkg",
    "dv-gateway": "file:../../dv-gateway",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.2",
    "nodemon": "^1.17.3",
    "protractor": "~5.1.2",
    "rxjs": "^5.5.6",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "zone.js": "^0.8.19"
  },
  "peerDependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "dv-core": "file:../../dv-core/pkg",
    "dv-gateway": "file:../../dv-gateway",
    "graphql": "^0.13.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  }
}
