{
  "name": "scoring",
  "gateway": {
    "config": {
      "wsPort": 3000
    }
  },
  "config": {
    "wsPort": 3001
  },
  "actions": {
    "package": {
      "include": [ "src/app/scoring/**" ]
    },
    "app": {
      "names": [
        { "for": "src/app/app.component.html", "use": "scoring-root" }
      ]
    }
  },
  "actionsRequestOptional": [
    "scoring-show-score",
    "scoring-show-target",
    "scoring-show-targets-by-score"
  ]
}
