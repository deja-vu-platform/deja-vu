{
  "name": "task",
  "gateway": {
    "config": {
      "wsPort": 3000
    }
  },
  "config": {
    "wsPort": 3001
  },
  "actions": {
    "package": {
      "include": [
        "src/app/task/**"
      ]
    },
    "app": {
      "names": [
        {"for": "src/app/app.component.html", "use": "task-root"}
      ]
    }
  },
  "actionsRequestOptional": [
    "task-show-task-count",
    "task-show-tasks"
  ]
}
