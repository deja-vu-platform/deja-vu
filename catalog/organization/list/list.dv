cliche List {
  data List {
    name: text,
    items: Item
  }

  data Item {
    name: text,
    checked: boolean
  }

  widget NewList {
  }

  widget EditList uses ShowList, AddItem {
    list: List
  }

  widget ShowList uses ShowItem {
    list: List
  }

  widget ShowItem {
    item: Item
  }

  widget ShowItemChecked {
    item: Item
  }

  widget EditItemChecked {
    item: Item
  }

  widget RedirectToList {
    list: List,
    on_redirect: Widget
  }

  widget RedirectToItem {
    item: Item,
    on_redirect: Widget
  }

  widget ShowListOverview {
    list: List
  }

  widget AddItem {
    item: Item,
    list: List,
    submit_ok: boolean
  }
}
