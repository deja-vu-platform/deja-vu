cliche Label {
  data Item {
    name: text,
    labels: Label
  }

  // TODO: Change once arrays work
  data ItemArr {
    arr: [Item]
  }

  data Label {
    name: text,
    items: Item
  }

  widget AttachLabels {
    item: Item,
    submit_ok: boolean
  }

  widget ShowLabels {
    item: Item
  }

  widget Search {
    items: ItemArr
  }

  widget ShowSearchResults uses ShowItem {
    items: ItemArr
  }

  widget ShowItem {
    item: Item
  }
}
