cliche Auth {
  data User {
    username: Text
  }
 
  // Will also sign in the user
  widget RegisterWithRedirect {
    register_ok_redirect: Widget
  }
  widget SignInWithRedirect {
    signin_ok_redirect: Widget
  }
  widget SignOutWithRedirect {
    signout_ok_redirect: Widget
  }
  widget LoggedIn {
    user: User
  }
}
