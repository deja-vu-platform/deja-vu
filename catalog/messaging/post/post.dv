cliche Post {
  data Post {
    content: Text,
    author: User
  }
  
  data User {
    username: Text
  }
  
  widget Post {
    post: Post
  }

  widget EditContent {
    post: Post
  }

  widget NewPostButton {
    post: Post
  }
  
  widget User {
    user: User
  }
  
  widget Posts {
    author: User
  }
  
  widget AllPosts {}
}
