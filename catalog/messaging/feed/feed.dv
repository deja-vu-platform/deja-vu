cliche Feed {
  data Publisher {
    name: Text,
    published: Message
  }

  data Subscriber {
    name: Text,
    subscriptions: Publisher
  }

  data Content {}

  data Message {
    content: Content
  }

  widget ShowFeed uses ShowFeedItem {
    subscriber: Subscriber
  }

  widget ShowFeedItem {
    message: Message,
    publisher: Publisher
  }
}
