cliche Feed {
  data Publisher {
    name: Text,
    published: Message
  }

  data Subscriber {
    name: Text,
    subscriptions: Publisher
  }

  data Content {}

  data Message {
    content: Content
  }

  widget ShowFeed {
    subscriber: Subscriber,
    feed_item_widget: ShowFeedItem
  }

  widget ShowFeedItem {}
}
