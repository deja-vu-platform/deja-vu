cliche Feed {
  data Publisher {
    name: Text,
    messages: Message
  }

  data Subscriber {
    name: Text,
    subscriptions: Publisher
  }

  data Message {
    content: Text
  }

  widget ShowFeed uses ShowFeedItem {
    sub: Subscriber
  }

  widget ShowFeedItem {
    sub: Subscriber,
    message: Message,
    publisher: Publisher
  }
}
