cliche Follow {
  data Source {
    follows: Target // array
  }

  data Target {
    name: text
  }

  widget EditFollow uses FollowUnfollow, ShowTarget {
    source: Source
  }

  widget FollowUnfollow {
    source: Source,
    target: Target
  }

  widget ShowTarget {
    target: Target
  }

  widget ShowFollowing uses ShowTarget {
    source: Source
  }
}
